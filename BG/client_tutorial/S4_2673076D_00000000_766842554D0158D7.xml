    <T n="text">0x4A7EC7A3<!--String: "Click to learn more about the Filter Panel."--></T>
    <T n="title">0xC3BBCC3D<!--String: "Filter Panel"--></T>
    <L n="ui_responses">
      <U>
        <T n="text">0xA13F0B8C<!--String: "Learn More   NOT USED"--></T>
