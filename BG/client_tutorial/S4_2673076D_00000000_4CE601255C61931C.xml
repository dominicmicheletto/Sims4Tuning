    <T n="text">0xE84BC264<!--String: "Click to learn more about the Household Inventory."--></T>
    <T n="title">0x2B8537A8<!--String: "Household Inventory"--></T>
    <L n="ui_responses">
      <U>
        <T n="text">0xA13F0B8C<!--String: "Learn More   NOT USED"--></T>
