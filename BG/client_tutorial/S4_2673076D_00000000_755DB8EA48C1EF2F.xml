  <U n="notification">
    <T n="icon" p="InGame\UI\Icons\HUD\HUD_BB_ByFunction.png">2f7d0004:00000000:ae023d4304570639</T>
    <T n="text">0x5A1F654E<!--String: "Click to learn more about Navigate Catalog by Function."--></T>
    <T n="title">0xBD40BA6E<!--String: "Objects by... Catalogs"--></T>
    <L n="ui_responses">
